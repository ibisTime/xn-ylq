		<result column="yqbj_total_amount" property="yqbjTotalAmount" />
		<result column="yqfy_total_amount" property="yqfyTotalAmount" />
	</resultMap>
	<sql id="where_condition">
		<trim prefix="WHERE" prefixOverrides="AND | OR">
			<if test="id != null and id != '' ">
				AND t.id = #{id}
			</if>
		</trim>
	</sql>
	<select id="select_statistic" parameterType="statistic"
		resultMap="statistic_DB_Result">
		SELECT
		t.id
		,t.today
		,t.reg_num
		,t.certi_num
		,t.zmxy_num
		,t.carrier_num
		,t.jdt_num
		,t.apply_num
		,t.fk_num
		,t.fk_amount
		,t.order_amount
		,t.yhk_num
		,t.yhk_amount
		,t.sjhk_num
		,t.bfhk_num
		,t.bfhk_amount
		,t.zfbhk_num
		,t.zfbhk_amount
		,t.hk_amount
		,t.xq_num
		,t.bfdk_num
		,t.bfdk_amount
		,t.zfbdk_num
		,t.zfbdk_amount
		,t.xq_total_amount
		,t.yqhk_num
		,t.yqhk_amount
		,t.bfyq_num
		,t.bfyq_total_amount
		,t.zfbyq_num
		,t.zfbyq_total_amount
		,t.ys_num
		,t.ys_total_amount
		,t.ysbj_total_amount
		,t.ysfy_total_amount
		,t.yq_count
		,t.yq_total_amount
		,t.yqbj_total_amount
		,t.yqfy_total_amount
		FROM
		t_statistic t
		<include refid="where_condition" />
		<trim prefix="ORDER BY ">
			<if test="order != null and order != '' ">
				t.${order}
			</if>
		</trim>
	</select>

	<select id="select_statistic_count" parameterType="statistic"
		resultType="java.lang.Long">
		SELECT count(1) FROM t_statistic t
		<include refid="where_condition" />
	</select>

	<insert id="insert_statistic" parameterType="statistic">
		INSERT INTO
		t_statistic(
		today
		,reg_num
		,certi_num
		,zmxy_num
		,carrier_num
		,jdt_num
		,apply_num
		,fk_num
		,fk_amount
		,order_amount
		,yhk_num
		,yhk_amount
		,sjhk_num
		,bfhk_num
		,bfhk_amount
		,zfbhk_num
		,zfbhk_amount
		,hk_amount
		,xq_num
		,bfdk_num
		,bfdk_amount
		,zfbdk_num
		,zfbdk_amount
		,xq_total_amount
		,yqhk_num
		,yqhk_amount
		,bfyq_num
		,bfyq_total_amount
		,zfbyq_num
		,zfbyq_total_amount
		,ys_num
		,ys_total_amount
		,ysbj_total_amount
		,ysfy_total_amount
		,yq_count
		,yq_total_amount
		,yqbj_total_amount
		,yqfy_total_amount
		)
		VALUES(
		#{today}
		,#{regNum}
		,#{certiNum}
		,#{zmxyNum}
		,#{carrierNum}
		,#{jdtNum}
		,#{applyNum}
		,#{fkNum}
		,#{fkAmount}
		,#{orderAmount}
		,#{yhkNum}
		,#{yhkAmount}
		,#{sjhkNum}
		,#{bfhkNum}
		,#{bfhkAmount}
		,#{zfbhkNum}
		,#{zfbhkAmount}
		,#{hkAmount}
		,#{xqNum}
		,#{bfdkNum}
		,#{bfdkAmount}
		,#{zfbdkNum}
		,#{zfbdkAmount}
		,#{xqTotalAmount}
		,#{yqhkNum}
		,#{yqhkAmount}
		,#{bfyqNum}
		,#{bfyqTotalAmount}
		,#{zfbyqNum}
		,#{zfbyqTotalAmount}
		,#{ysNum}
		,#{ysTotalAmount}
		,#{ysbjTotalAmount}
		,#{ysfyTotalAmount}
		,#{yqCount}
		,#{yqTotalAmount}
		,#{yqbjTotalAmount}
		,#{yqfyTotalAmount}
		)
	</insert>

